# Database connection parameters
DB_HOST=
DB_PORT=
DB_SERVICE=
DB_USER=
DB_PWD=
DB_POOL_MIN=2
DB_POOL_MAX=6

# Vector search parameters
DISTANCE_MATRIC=COSINE
NEAREST_TOPN=3
DISTANCE_THRESHOLD=0.25
DISTANCE_THRESHOLD_ACCURATE=0.05

# SelectAI parameters
SELECTAI_PROFILE=

# Data security with Oracle VPD
ENABLE_VPD=FALSE

# Application server
SERVER_HOST=0.0.0.0
SERVER_LISTEN_PORT=8080
CONTEXT_ROOT=/aireport

# OCI Generative AI Service configuration
OCI_GENAI_COMPARTMENT=
OCI_CONFIG_FILE=~/.oci/config
OCI_CONFIG_PROFILE=GENERATEAI
OCI_SERVICE_ENDPOINT=https://inference.generativeai.us-chicago-1.oci.oraclecloud.com
OCI_GENAI_MODEL=meta.llama-3.1-70b-instruct
EMBEDDING_MODEL=cohere.embed-multilingual-v3.0

# Application debug level
DEBUG_LEVEL=10
LOG_FILE_PATH=logs/text2sql-rag.log
